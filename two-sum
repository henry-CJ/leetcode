/*
  问题描述：从给定的数组 nums 中，找到其中 2 个能相加和等于 target 的，并将这两个数的索引放到数组里并返回
 */
/**
 * @param {number[]} nums
 * @param {number} target
 * @return {number[]}
 */
var twoSum = function(nums, target) {
    for (let i=nums.length;i--;) { // 倒序循环减少计算步骤
        for (let j=i;j--;) {
            if (nums[i]+nums[j]==target){
                return [i,j];
            }
        }
    }
};
